{
  "name": "reactnativeseed",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": " react-native run-android",
    "ios": " react-native run-ios",
    "lint": "eslint .",
    "start": " react-native start",
    "test": "jest",
    "tc": "tsc",
    "clean": "watchman watch-del-all  && cd android && ./gradlew clean && cd ..",
    "build:debug:android": "yarn react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && cd android && ./gradlew assembleDebug",
    "build:release:android": "cd android && ./gradlew assembleRelease",
    "clean:cache:android": "cd android && ./gradlew cleanBuildCache && cd ..",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'",
    "deeplink:open:android": "npx uri-scheme open 'reactnativeseed://' --android",
    "deeplink:open:ios": "npx uri-scheme open 'reactnativeseed://' --ios"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@react-native-community/netinfo": "11.2.1",
    "@react-navigation/devtools": "^6.0.20",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@react-navigation/stack": "^6.3.20",
    "@reduxjs/toolkit": "^2.0.1",
    "@types/react-native-vector-icons": "^6.4.18",
    "babel-plugin-module-resolver": "^5.0.0",
    "dayjs": "^1.11.10",
    "react": "18.2.0",
    "react-hook-form": "^7.49.2",
    "react-native": "0.73.0",
    "react-native-device-info": "^10.12.0",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "2.14.0",
    "react-native-modal": "^13.0.1",
    "react-native-paper": "^5.11.4",
    "react-native-paper-dates": "^0.20.5",
    "react-native-safe-area-context": "^4.8.1",
    "react-native-safe-area-view": "^1.1.1",
    "react-native-screens": "3.29.0",
    "react-native-simple-toast": "^3.1.0",
    "react-native-snackbar": "^2.6.2",
    "react-native-vector-icons": "^10.0.3",
    "react-redux": "^9.0.4",
    "redux-persist": "^6.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@babel/runtime": "^7.23.6",
    "@react-native/babel-preset": "^0.74.0",
    "@react-native/eslint-config": "^0.74.0",
    "@react-native/metro-config": "^0.73.2",
    "@react-native/typescript-config": "^0.74.0",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
